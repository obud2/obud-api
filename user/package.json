{
  "name": "user-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "dev": "nodemon --exec node --loader ts-node/esm serverLocal.ts",
    "delete": "claudia destroy --profile obud",
    "deploy": "npx tsc  && cp -f *-config.json dist/ && claudia create --profile obud --runtime nodejs18.x --memory 512 --policies policies --use-s3-bucket claudia.obud.site --handler index.handler --deploy-proxy-api --region ap-northeast-2",
    "update-S3": "npx tsc && cp -f *-config.json dist/ && claudia update --profile obud  --memory 512 --use-s3-bucket claudia.obud.site  --region ap-northeast-2"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.347.1",
    "@aws-sdk/client-dynamodb": "^3.342.0",
    "@aws-sdk/credential-providers": "^3.226.0",
    "@types/nodemailer": "^6.4.8",
    "atoz-jthor": "^2.0.0",
    "aws-iot-device-sdk-v2": "^1.9.5",
    "aws-serverless-express": "^3.3.8",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.13.0",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-apple": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-naver": "^1.0.6",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.7",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.12",
    "@types/passport-apple": "^1.1.1",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-naver": "^1.0.1",
    "claudia": "^5.13.1",
    "nodemon": "^2.0.22",
    "swagger-jsdoc": "^6.2.7",
    "swagger-ui-express": "^4.6.0",
    "typescript": "^4.9.4"
  }
}
